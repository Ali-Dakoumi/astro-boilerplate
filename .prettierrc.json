{
	"useTabs": true,
	"singleQuote": true,
	"semi": false,
	"tabWidth": 2,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": false,
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"printWidth": 120,
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
